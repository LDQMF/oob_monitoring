[Unit]
Description=oob_monitoring
After=network.target

[Service]
User=cchiang
Restart=always
StartLimitBurst=0
Environment=SYSTEMD_LOG_LEVEL=debug
RestartSec=5
WorkingDirectory=/home/cchiang/oob_monitoring
ExecStart=/usr/bin/env python3 daemon.py
TimeoutStopSec=3
StandardOutput=tty
StandardInput=tty

[Install]
WantedBy=multi-user.target
